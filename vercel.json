{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node"}],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_PASSWORD": "@agenda-db-pass",
    "DB_USER": "@agenda-db-user",
    "DB_NAME": "@agenda-db-name",
    "DB_HOST": "@agenda-db-host",
    "CORS": "*",
    "AUTH_ADMIN_USERNAME": "@agenda-auth-user",
    "AUTH_ADMIN_PASSWORD": "@agenda-auth-pass",
    "AUTH_ADMIN_EMAIL": "@agenda-auth-email",
    "AUTH_JWT_SECRET": "@agenda-jwt-secret",
    "SECRET_ACCESS_KEY": "@agenda-secret-key",
    "ACCESS_KEY_ID": "@agenda-access-key"
  }
}